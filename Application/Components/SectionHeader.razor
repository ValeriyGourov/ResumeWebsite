<header class="o-section__header  t-section__header">
	<div class="o-content">

		<h2 class="o-section__heading">
			@Title
		</h2>
		<div class="o-content__body  o-section__description">
			@Description
		</div>

	</div>
</header>

@code{
	[Parameter]
	public string Title { get; set; } = null!;

	[Parameter]
	public string Description { get; set; } = null!;
}
