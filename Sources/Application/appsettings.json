{
	"Serilog": {
		"MinimumLevel": {
			"Default": "Information",
			"Override": {
				"Microsoft": "Information",
				"Microsoft.AspNetCore": "Warning", // Для использования Serilog request logging.
				"Microsoft.AspNetCore.HttpLogging.HttpLoggingMiddleware": "Information"
			}
		}
	},
	"AllowedHosts": "*"
}
